{
    "manifest_version": 3,
    "name": "Quick Links New Tab",
    "version": "1.0",
    "description": "A new tab extension for saving quick links",
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "permissions": [
        "storage"
    ],
    "action": {
        "default_popup": "index.html"
    }
}